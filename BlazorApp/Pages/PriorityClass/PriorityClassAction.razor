@using BlazorApp.Pages.Common
@using k8s.Models
<ShareMenuItem T="V1PriorityClass" Item="Item" MenuMode="MenuMode">
    @if (MenuMode == MenuMode.Vertical)
    {
        <MenuItem OnClick="@(() => OnCancelDefaultClick(Item))">CancelDefault</MenuItem>

    }
    @if (MenuMode == MenuMode.Horizontal)
    {
        <Button OnClick="@(() => OnCancelDefaultClick(Item))">CancelDefault</Button>
    }
</ShareMenuItem>
