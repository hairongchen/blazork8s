@using k8s.Models
@using BlazorApp.Pages.Common.Metadata
@using BlazorApp.Pages.Event
@using BlazorApp.Pages.Common;
@inherits DrawerPageBase<V1StatefulSet>
@if (StatefulSet != null)
{
    <StatefulSetAction Item="StatefulSet" MenuMode="MenuMode.Horizontal"  ></StatefulSetAction>
    <Divider Orientation="left" Style="font-weight:bold">Metadata</Divider>
    <MetadataView Item="StatefulSet" T="V1StatefulSet" ExplainFieldPrefix="statefulSet"></MetadataView>
    <Divider Orientation="left" Style="font-weight:bold">Spec</Divider>
    <Descriptions Bordered Column="1">
        <DescriptionsItem Title="Name">
            @StatefulSet.Name()
        </DescriptionsItem>
                <PropertySimpleView Title="MinReadySeconds" Item="@StatefulSet.Spec.MinReadySeconds" ExplainField="statefulSet.spec.minReadySeconds" ></PropertySimpleView>
                <PropertySimpleView Title="Ordinals" Item="@StatefulSet.Spec.Ordinals" ExplainField="statefulSet.spec.ordinals"  ShowInJson="true" ></PropertySimpleView>
                <PropertySimpleView Title="PersistentVolumeClaimRetentionPolicy" Item="@StatefulSet.Spec.PersistentVolumeClaimRetentionPolicy" ExplainField="statefulSet.spec.persistentVolumeClaimRetentionPolicy"  ShowInJson="true" ></PropertySimpleView>
                <PropertySimpleView Title="PodManagementPolicy" Item="@StatefulSet.Spec.PodManagementPolicy" ExplainField="statefulSet.spec.podManagementPolicy" ></PropertySimpleView>
                <PropertySimpleView Title="Replicas" Item="@StatefulSet.Spec.Replicas" ExplainField="statefulSet.spec.replicas" ></PropertySimpleView>
                <PropertySimpleView Title="RevisionHistoryLimit" Item="@StatefulSet.Spec.RevisionHistoryLimit" ExplainField="statefulSet.spec.revisionHistoryLimit" ></PropertySimpleView>
                <PropertySimpleView Title="Selector" Item="@StatefulSet.Spec.Selector" ExplainField="statefulSet.spec.selector"  ShowInJson="true" ></PropertySimpleView>
                <PropertySimpleView Title="ServiceName" Item="@StatefulSet.Spec.ServiceName" ExplainField="statefulSet.spec.serviceName" ></PropertySimpleView>
                <PropertySimpleView Title="Template" Item="@StatefulSet.Spec.Template" ExplainField="statefulSet.spec.template"  ShowInJson="true" ></PropertySimpleView>
                <PropertySimpleView Title="UpdateStrategy" Item="@StatefulSet.Spec.UpdateStrategy" ExplainField="statefulSet.spec.updateStrategy"  ShowInJson="true" ></PropertySimpleView>
                <PropertySimpleView Title="VolumeClaimTemplates" Item="@StatefulSet.Spec.VolumeClaimTemplates" ExplainField="statefulSet.spec.volumeClaimTemplates" ShowInJson="true"></PropertySimpleView>
    </Descriptions>
 <Divider Orientation="left" Style="font-weight:bold">Status</Divider>
    <Descriptions Bordered Column="1">
                <PropertySimpleView Title="AvailableReplicas" Item="@StatefulSet.Status.AvailableReplicas" ExplainField="statefulSet.status.availableReplicas" ></PropertySimpleView>
                <PropertySimpleView Title="CollisionCount" Item="@StatefulSet.Status.CollisionCount" ExplainField="statefulSet.status.collisionCount" ></PropertySimpleView>
                <PropertySimpleView Title="Conditions" Item="@StatefulSet.Status.Conditions" ExplainField="statefulSet.status.conditions" ShowInJson="true"></PropertySimpleView>
                <PropertySimpleView Title="CurrentReplicas" Item="@StatefulSet.Status.CurrentReplicas" ExplainField="statefulSet.status.currentReplicas" ></PropertySimpleView>
                <PropertySimpleView Title="CurrentRevision" Item="@StatefulSet.Status.CurrentRevision" ExplainField="statefulSet.status.currentRevision" ></PropertySimpleView>
                <PropertySimpleView Title="ObservedGeneration" Item="@StatefulSet.Status.ObservedGeneration" ExplainField="statefulSet.status.observedGeneration" ></PropertySimpleView>
                <PropertySimpleView Title="ReadyReplicas" Item="@StatefulSet.Status.ReadyReplicas" ExplainField="statefulSet.status.readyReplicas" ></PropertySimpleView>
                <PropertySimpleView Title="Replicas" Item="@StatefulSet.Status.Replicas" ExplainField="statefulSet.status.replicas" ></PropertySimpleView>
                <PropertySimpleView Title="UpdateRevision" Item="@StatefulSet.Status.UpdateRevision" ExplainField="statefulSet.status.updateRevision" ></PropertySimpleView>
                <PropertySimpleView Title="UpdatedReplicas" Item="@StatefulSet.Status.UpdatedReplicas" ExplainField="statefulSet.status.updatedReplicas" ></PropertySimpleView>
    </Descriptions>
    <MiniEventListView Uid="@StatefulSet.Metadata.Uid"></MiniEventListView>
}