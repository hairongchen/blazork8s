@using BlazorApp.Pages.Common.Html.HTable
@using BlazorApp.Pages.Common.Html
<Divider Orientation="left" Style="font-weight:bold">@BlockTitle</Divider>
<HTable Bordered Size="small">
    <HTableColumn Style="width:20px" Title="CPU">@Dict["cpu"].ToString()</HTableColumn>
    <HTableColumn Title="Memory">
        @Echo("memory")
    </HTableColumn>
    <HTableColumn Title="Ephemeral Storage">
        @Echo("ephemeral-storage")
    </HTableColumn>
    <HTableColumn Title="Hugepages-1Gi">
        @Echo("hugepages-1Gi")
    </HTableColumn>
    <HTableColumn Title="Hugepages-2Mi">
        @Echo("hugepages-2Mi")
    </HTableColumn>
    <HTableColumn Title="Pods"> @Dict["pods"].ToString()</HTableColumn>
</HTable>
