@if (Owner != null)
{
    @foreach (var o in Owner)
    {
        <Tag PresetColor=@PresetColor.Green
             OnClick="@(() => OnObjClick(o))"
             Style="cursor: pointer">
            @o.Kind &nbsp;
            @if (ShowOwnerName)
            {
                @o.Name
            }
        </Tag>
    }
}
