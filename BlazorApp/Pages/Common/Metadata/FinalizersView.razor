@using BlazorApp.Pages.Common.Html
@inherits BlazorApp.Pages.Common.PageBase

@if (Finalizers != null)
{
    <HTableColumn Title=@L["Finalizers"]>
        @foreach (var label in Finalizers)
        {
            <Paragraph Copyable>
                <ColorfulTag>@label</ColorfulTag>
            </Paragraph>
        }
    </HTableColumn>
}
