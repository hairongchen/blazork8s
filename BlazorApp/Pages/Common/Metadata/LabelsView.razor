@using BlazorApp.Utils
@if (Labels != null)
{
    <DescriptionsItem Title="Labels">
        @foreach (var label in Labels)
        {
            <Paragraph Copyable>
                <Tag Color="@RandomColorHelper.RandomInverseColor()">@label.Key = @label.Value</Tag>
            </Paragraph>
        }
    </DescriptionsItem>
}
