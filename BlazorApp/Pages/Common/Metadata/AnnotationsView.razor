@using BlazorApp.Pages.Common.Html
@inherits BlazorApp.Pages.Common.PageBase

@if (Annotations is { Count: > 0 })
{
    <HTableColumn Title=@L["Annotations"] ExplainField="@ExplainField">
        @foreach (var label in Annotations)
        {
            <Paragraph >
                <Text Code Copyable>
                    <span >@label.Key = @label.Value</span>
                </Text>
            </Paragraph>
        }
    </HTableColumn>
}
