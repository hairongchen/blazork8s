@using BlazorApp.Pages.Common.Html
@if (Annotations != null && Annotations.Count > 0)
{
    <HTableColumn Title="Annotations">
        @foreach (var label in Annotations)
        {
            <Paragraph >
                <Text Code Copyable>
                    <span  >@label.Key = @label.Value</span>
                </Text>
            </Paragraph>
        }
    </HTableColumn>
}
