@using BlazorApp.Pages.Common.Html
@using BlazorApp.Utils
@inherits PageBase

@if (Selector is { Keys.Count: > 0 })
{
    <HTableColumn Title="@Title" ExplainField="@ExplainField">
        @foreach (var label in Selector)
        {
            <Tag Color=@RandomColorHelper.RandomColor()>@label.Key = @label.Value</Tag>
        }
    </HTableColumn>
}
