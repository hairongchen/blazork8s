@using BlazorApp.Pages.Common.Html
@using k8s.Models
@inherits BlazorApp.Pages.Common.PageBase
@if (Backend != null)
{
    <HTableColumn Title=@Title ExplainField="@ExplainField">
        <IngressBackendNoTableView Namespace="@Namespace" Backend="@Backend"></IngressBackendNoTableView>
    </HTableColumn>
}


@code {

    [Parameter] public V1IngressBackend Backend { get; set; }

    [Parameter] public string Title { get; set; }
    [Parameter] public string ExplainField { get; set; }
    [Parameter] public string Namespace { get; set; }

}
