@using k8s.Models
@using BlazorApp.Pages.Common.Metadata
@if (Fc != null)
{
    <PropertyListView Title="TargetWWNs" ExplainField=@(FieldPrefix+".fc.targetWWNs") Items="@Fc.TargetWWNs" T="string"></PropertyListView>
    <PropertyListView Title="Wwids" ExplainField=@(FieldPrefix+".fc.wwids") Items="@Fc.Wwids" T="string"></PropertyListView>
    <PropertySimpleView Title="Lun" ExplainField=@(FieldPrefix+".fc.lun") Item="@Fc.Lun"></PropertySimpleView>
    <PropertySimpleView Title="FsType" ExplainField=@(FieldPrefix+".fc.fsType") Item="@Fc.FsType"></PropertySimpleView>
    <PropertySimpleView Title="ReadOnly" ExplainField=@(FieldPrefix+".fc.readOnly") Item="@Fc.ReadOnlyProperty"></PropertySimpleView>
}


@code {

    [Parameter]
    public V1FCVolumeSource Fc { get; set; }
    [Parameter]
    public string FieldPrefix { get; set; }
}
